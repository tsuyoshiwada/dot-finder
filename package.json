  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsuyoshiwada/dot-finder/issues"
  },
  "homepage": "https://github.com/tsuyoshiwada/dot-finder#readme",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-register": "^6.22.0",
    "eslint": "^3.13.1",
    "eslint-plugin-babel": "^4.0.1",
    "mocha": "^3.2.0",
    "power-assert": "^1.4.2"
